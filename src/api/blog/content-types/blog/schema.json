{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "createAt": {
      "type": "datetime"
    },
    "seo": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false
    },
    "hero": {
      "type": "component",
      "component": "blog.blog-hero-section",
      "repeatable": false
    },
    "sections": {
      "type": "dynamiczone",
      "components": [
        "blog.blog-content-section",
        "blog.blog-faq-section"
      ]
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "inversedBy": "blogs"
    }
  }
}
