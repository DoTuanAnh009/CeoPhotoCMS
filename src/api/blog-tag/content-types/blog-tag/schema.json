{
  "kind": "collectionType",
  "collectionName": "blog_tags",
  "info": {
    "singularName": "blog-tag",
    "pluralName": "blog-tags",
    "displayName": "Blog Tag"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "description": {
      "type": "string"
    },
    "posts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog-post.blog-post",
      "mappedBy": "tags"
    }
  }
}
